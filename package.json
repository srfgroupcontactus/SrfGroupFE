  "scripts": {
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "start:dev": "env-cmd -f ./environments/.dev.env react-scripts start",
    "build": "env-cmd -f ./environments/.prod.env npm run build:prod:windows",
    "build:prod:windows-with-source-map": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "build:prod:windows": "react-scripts build",
    "build:prod": "env-cmd -f ./environments/.prod.env npm run build:prod:windows",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage",
    "test-sonar": "react-scripts test --watchAll=false --coverage --testResultsProcessor jest-sonar-reporter",
    "eject": "react-scripts eject",
    "serve:build:prod": "npm run build:prod && serve -s build",
    "generate-sonar-rapport": "node sonar-project.js",
    "docker:elasticsearch:up": "docker-compose -f docker/elasticsearch.yml up -d",
    "docker:elasticsearch:down": "docker-compose -f docker/elasticsearch.yml down -v --remove-orphans",
    "docker:db:up": "docker-compose -f docker/postgresql.yml up -d",
    "docker:db:down": "docker-compose -f docker/postgresql.yml down -v --remove-orphans",
    "docker:kibana:up": "docker-compose -f docker/kibana.yml up",
    "docker:kibana:down": "docker-compose -f docker/kibana.yml down -v --remove-orphans",
    "docker:sonar:up": "docker-compose -f docker/sonar.yml up -d",
    "docker:sonar:down": "docker-compose -f docker/sonar.yml down -v --remove-orphans",
    "docker:logstash:up": "docker-compose -f docker/logstash.yml up -d",
    "docker:logstash:down": "docker-compose -f docker/logstash.yml down -v --remove-orphans",
    "docker:react:up": "docker-compose -f react.yml up ",
    "docker:react:down": "docker-compose -f react.yml down -v --remove-orphans",
    "clear-jest": "jest --clearCache",
    "prettier": "prettier  --write src/**/*.{ts,tsx}",
    "lint:ts": "npm run prettier && eslint --fix src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
