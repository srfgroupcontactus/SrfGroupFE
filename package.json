  "jest": {
    "transform": {
      ".+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@mui)/",
      "node_modules/(?!swiper)/",
      "node_modules/(?!react-facebook-login)/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
